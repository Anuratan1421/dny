{
    "version": 2,
    "builds": [
      {
        "src": "Server/index.js", 
        "use": "@vercel/node"
      },
      {
        "src": "Client/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "Server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/Client/$1"
      }
    ]
  }
  